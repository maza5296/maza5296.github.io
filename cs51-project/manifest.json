{
  "manifest_version": 2,

  "name": "AES Password Manager",
  "description": "This extension encrypts and decrypts passwords for the current display page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/Key.png",
    "default_popup": "index.html",
    "default_title": "AES Password Manager"
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
